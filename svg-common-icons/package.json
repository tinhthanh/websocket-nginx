{
  "name": "svg-common-icons",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve  --host 0.0.0.0 --disable-host-check",
    "build": "ng build --prod",
    "build-prod": "ng build",
    "watch": "ng build --watch --configuration development",
